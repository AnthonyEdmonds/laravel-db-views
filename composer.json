{
    "name": "anthonyedmonds/laravel-db-views",
    "description": "Manage database views for Laravel applications",
    "keywords": ["laravel", "database", "views"],
    "type": "library",
    "homepage": "https://github.com/AnthonyEdmonds/laravel-db-views",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Edmonds",
            "email": "contact@anthonyedmonds.me",
            "homepage": "https://anthonyedmonds.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8",
        "illuminate/support": "^12"
    },
    "require-dev": {
        "brianium/paratest": "^7",
        "larastan/larastan": "^3",
        "laravel/pint": "^1",
        "networkrailbusinesssystems/badges": "^1",
        "orchestra/testbench": "^10",
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelDbViews\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnthonyEdmonds\\LaravelDbViews\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnthonyEdmonds\\LaravelDbViews\\DbViewsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
